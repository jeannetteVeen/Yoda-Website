<script data-cfasync="false">
URL = window.URL || window.webkitURL;

$(document).ready(function(){
	
	let recordingIsActive = false;
	let remainingTime = 0;
	let gumStream;
	let recorder;
	let input;
	let AudioContext = window.AudioContext || window.webkitAudioContext;
	let audioContext;
	
	voiceRecord = function(type, recTimer) {
		recorder = undefined;
		const constraints = {audio: true, video: false};
		navigator.mediaDevices.getUserMedia(constraints).then(stream => {
			if (recordingIsActive) return false;
			recordingIsActive = true;
			audioContext = new AudioContext();
			gumStream = stream;
			input = audioContext.createMediaStreamSource(stream);
			recorder = new WebAudioRecorder(input, {
				workerDir: "addons/voice_record/files/",
				encoding: 'mp3',
				numChannels: 2,
				onEncoderLoading: (recorder, encoding) => {
				},
				onEncoderLoaded: (recorder, encoding) => {
				}
			});
			recorder.onComplete = (recorder, blob) => {
				var formData = new FormData();
				formData.append('file', blob);
				formData.append('token', utk);
								if(type == 'private'){
					formData.append('target', currentPrivate);
					formData.append('private', 1);
				}
								$.ajax({
					url: 'addons/voice_record/system/blob_chat.php',
					dataType: 'json',
					cache: false,
					method: "POST",
					data: formData,
					processData: false,
					contentType: false,
					success: function (response) {
						if(response.code == 1){
							callError(system.wrongFile);
						}
						else if(response.code == 5){
							if(type == 'private'){
								appendSelfPrivateMessage(response.logs);
							}
							else {
								appendSelfChatMessage(response.logs);
							}
						}
						else {
							callError(system.error);
						}
						$('#record_'+type).removeClass('fa-circle-o-notch fa-spin fa-fw');
						$('#record_'+type).addClass('fa-microphone');
						$('#record_'+type).attr('data', 0);
					},
					error: function () {
						callError(system.error);
					}
				});
			};
			recorder.setOptions({
				timeLimit: recTimer,
				encodeAfterRecord: true,
				mp3: {bitRate: 96}
			});
			recorder.startRecording();
			if (recTimer !== 0) {
				const time_callback = _RemainingTime => {
					if (_RemainingTime === 0 && recordingIsActive) {
						stopRecording(type);
					}
				};
				voiceTimer(recTimer, time_callback);
			}
			$('#record_'+type).attr('data', 1);
			$('#record_'+type).removeClass('fa-microphone');
			$('#record_'+type).addClass('fa-circle voice_recording');

		}).catch((errors) => {
			console.log(errors);
			alert('Please allow microphone access!');
		});

	}
	stopRecording = function(type) {
		remainingTime = 0;
		recordingIsActive = false;
		$('#record_'+type).removeClass('fa-circle voice_recording');
		$('#record_'+type).addClass('fa-circle-o-notch fa-spin fa-fw');
		gumStream.getAudioTracks()[0].stop();
		recorder.finishRecording();
	}
	voiceTimer = function(seconds, cb) {
		remainingTime = seconds;
		setTimeout(() => {
			cb(remainingTime);
			if (remainingTime > 0) {
				voiceTimer(remainingTime - 1, cb);
			}
		}, 1000);
	}
	runVoiceRecord = function(item){
		var s = $(item).attr('data');
		var t = $(item).attr('data-type');
		if(s == 0){
			if(t == 'chat'){
				var m = 30;
			}
			if(t == 'private'){
				var m = 30;
			}
			voiceRecord(t, m);
		}
		else {
			stopRecording(t);
		}
	}

			$('<div class="input_item main_item base_main"><i class="fa fa-microphone" data="0" data-type="private" id="record_private"></i>').insertAfter('#private_input_box');
		$(document).on("click", "#record_chat, #record_private", function(){ 
		runVoiceRecord($(this));
	});
	boomAddCss('addons/voice_record/files/voice_record.css');
	
});
</script>
<script data-cfasync="false" src="addons/voice_record/files/WebAudioRecorder.min.js"></script>
<script data-cfasync="false" type="text/javascript">
$(document).ready(function() {
		    appInputMenu('addons/profile_music/files/icon.png', 'profileMusicBox();');
		profileMusicBox = function() {
			$.post('addons/profile_music/system/action.php', {
				edit_profile_music: 1,
				token: utk,
			}, function(response) {
				showModal(response, 500);
				$("#fileuploader").uploadFile({
					url: 'addons/profile_music/system/action.php',
					fileName:"file",
					multiple:false,
					dragDrop:false,
					maxFileCount:1,
					uploadStr: '<i class="fa fa-paperclip"></i> Select music file',
					formData: { 
						save_profile_music: 1, 
						token:utk 
					},
					onSubmit:function(files) {
						$(".upload_label").hide();
						$("#fileuploader").hide();
					  },
					onSuccess:function(files,data,xhr,pd){
						callSaved(system.saved, 1);
						profileMusicBox();
					},
				});
			});
		}
		deleteProfileMusic = function() {
			$.post('addons/profile_music/system/action.php', {
				remove_profile_music: 1,
				token: utk,
			}, function(response) {
				callSaved(system.saved, 1);
				profileMusicBox();
			});
		}
		
    getProfileMusic = function(t) {
		$.post('addons/profile_music/system/action.php', {
				get_profile_music: 1,
				target: t,
			}, function(response) {
				if(response == ''){
					return false;
				}
				$('#probio').prepend('<div class="btable blisting proitem" id="profile_music"></div>');
				$('#profile_music').html(response); 
			});
	}

	$(document).on('click','.get_info', function(){
		var target = $(this).attr('data');
		getProfileMusic(target);
	});
    
});

